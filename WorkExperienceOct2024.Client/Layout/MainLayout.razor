@inherits LayoutComponentBase

<Bar Class="rdt-brand-bar">
    <BarBrand>
        <img src="data:image/png;base64," class="rdt-logo" />
    </BarBrand>
    <BarItem>
        <a href="https://rdt.co.uk/" target="_blank" class="px-3">About</a>
    </BarItem>
</Bar>

<div class="page">
    <div class="sidebar">
        <NavMenu />
    </div>
    <main>
        <article class="content px-4">
            @Body
        </article>
    </main>
</div>

@{
    <style scoped>
        .rdt-brand-bar {
            background: #eb5f0a;
            border-bottom: solid #c63e01;
            height: 75px !important;
        }

        .rdt-logo {
            height: 60px !important;
            margin-left: 25px;
        }
    </style>
}